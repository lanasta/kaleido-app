{
  "name": "kaleido-app",
  "version": "1.0.0",
  "description": "Kaleido Front End Challenge App",
  "author": {
    "name": "Anastasia Lalamentik",
    "url": "https://lanasta.github.io"
  },
  "license": "MIT",
  "scripts": {
    "client": "cd client && yarn start",
    "server": "nodemon server.js",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
    "dev:server": "cd client && yarn build && cd .. && yarn start",
    "start": "node server.js",
    "node-sass": "node-sass --output-style compressed -o dist/css src/scss"
  },
  "dependencies": {
    "eject": "0.0.4",
    "express": "^4.16.3",
    "highcharts": "^6.2.0",
    "html-react-parser": "^0.4.7"
  },
  "devDependencies": {
    "concurrently": "^4.0.1",
    "node-sass": "^4.9.4",
    "sass-loader": "^7.1.0"
  },
  "keywords": [
    "node",
    "express",
    "create-react-app"
  ]
}
